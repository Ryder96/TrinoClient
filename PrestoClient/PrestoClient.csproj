<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>PrestoClient</AssemblyName>
    <RootNamespace>BAMCIS.PrestoClient</RootNamespace>
    <Version>1.0.0.0</Version>
    <Authors>Michael Haken</Authors>
    <Company>bamcis.io</Company>
    <PackageReleaseNotes>This is a very alpha version of a .NET client for Prestodb. It was developed against Presto version 0.196 and 0.197.</PackageReleaseNotes>
    <Copyright>2018 BAMCIS</Copyright>
    <PackageProjectUrl>https://github.com/bamcis-io/PrestoClient</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.4.1" />
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Model\SPI\Predicate\" />
  </ItemGroup>

</Project>
